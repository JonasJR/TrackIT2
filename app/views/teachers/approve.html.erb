<div id="approve">
    <h2>Browse
        <span class="spans">I</span>t</h2>
    <table class="table table-hover" id="approve-proposals">
        <thead>
            <tr>
                <th>Name</th>
                <th>Description</th>
                <th>Category</th>
                <th>Approve?</th>
                <th>Remove?</th>
            </tr>
        </thead>
        <tbody>
            <% @proposals.each do |p| %>
            <tr data-link="<%= proposal_path(p) %>">
                <td class="col-md-3"><%= p.name %></td>
                <td class="col-md-4"><%= p.description %></td>
                <td class="col-md-3"><%= p.category %></td>
                <td class="col-md-1"><i class="glyphicon glyphicon-ok"></i></td>
                <td class="col-md-1"><i class="glyphicon glyphicon-remove"></i></td>
            </tr>
            <% end %>
        </tbody>
    </table>
</div>
<script type="text/javascript">
$("tr").click(function() {
  window.location = $(this).data("link")
});
</script>
